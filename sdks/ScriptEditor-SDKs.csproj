<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <RootNamespace>ScriptEditor_SDKs</RootNamespace>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Conholdate.Total" Version="25.4.0" />
        <PackageReference Include="DotNetZip" Version="1.16.0" />
        <PackageReference Include="IronZip" Version="2025.4.3" />
    </ItemGroup>
    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
        <OutputPath>dist\csharp\Debug</OutputPath>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
        <OutputPath>dist\csharp\Release</OutputPath>
    </PropertyGroup>
</Project>